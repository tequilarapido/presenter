{
  "name": "tequilarapido/presenter",
  "description": "Simple and straightforward presenter implementation for Eloquent models.",
  "homepage": "https://github.com/tequilarapido/presenter",
  "license": "MIT",
  "authors": [
    {
      "name": "Nassif Bourguig",
      "email": "nbourguig@gmail.com",
      "homepage": "https://github.com/nbourguig",
      "role": "Developer"
    }
  ],
  "require": {
    "laravel/framework": ">=5.8 <=6.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Tequilarapido\\Presenter\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tequilarapido\\Presenter\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1-dev"
    }
  }
}
